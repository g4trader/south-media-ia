{
  "version": 2,
  "buildCommand": "echo 'Static site'",
  "outputDirectory": "dashboard",
  "framework": null,
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
