{
  "version": 2,
  "buildCommand": "echo 'Static site'",
  "outputDirectory": ".",
  "framework": null,
  "routes": [
    {
      "src": "/",
      "dest": "/dash_sonho.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dash_sonho.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
